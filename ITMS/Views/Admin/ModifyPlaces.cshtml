@model IEnumerable<tblPlaces>
<script>
                @{
                if (@ViewData["Successful"] != null)
                {
                    <Text>
                    var result = '@Html.Raw(ViewData["Successful"])';
                    Swal.fire(result,'','success')
                    </Text>
                    }
                   else if(@ViewData["Falied"] != null )
                    {
                    <Text>
                        var result = '@Html.Raw(ViewData["Falied"])';
                        Swal.fire(result,'','error')
                    </Text>
                    }
            }
</script>
<div class="card-body">
    <div id="v-card" class="row justify-content-center" style="margin-top: 44px;">
        <div class="col-md-7 ">
            <a asp-action="AddPlace" class="btn btn-outline-primary"> AddPlace</a>
            <table id="datatablesSimple">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Place Name</th>
                        <th>City</th>
                        <th>Action</th>

                    </tr>
                </thead>
                <tbody>
                    @{ var i = 1;
                        foreach (var item in Model)
                        {

                            <tr>
                                <td>@i </td>
                                <td>@item.Name</td>
                                <td>@item.City.CityName</td>
                                <td>
                                    <a asp-controller="Admin" asp-action="EditPlace" asp-route-id="@item.GuId">Edit</a> |
                                    <a asp-controller="Admin" asp-action="DeletePlace" asp-route-id="@item.GuId">Delete</a>
                                </td>
                            </tr>
                            i++;
                        } }
                </tbody>
            </table>
        </div>
    </div>
</div>