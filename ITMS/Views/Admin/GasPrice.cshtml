<script>
                @{
                if (@ViewData["Successful"] != null)
                {
                    <Text>
                    var result = '@Html.Raw(ViewData["Successful"])';
                    Swal.fire(result,'','success')
                        .then(
                            function () {
                                window.location.href = '@Url.Action("Dashboard", "Admin")';
                            }
                        );
                    </Text>
                    }
                   else if(@ViewData["Falied"] != null )
                    {
                    <Text>
                        var result = '@Html.Raw(ViewData["Falied"])';
                        Swal.fire(result,'','error')
                        .then(
                            function () {
                                window.location.href = '@Url.Action("Dashboard", "Admin")';
                            }
                        );
                    </Text>
                    }
            }
</script>
<div class="container">
    <div id="v-card" class="d-flex justify-content-center" style="margin-top: 100px;">
        <div class="card col-md-6">
            <div class="card-header">
                <h3>Gas Prices</h3>
            </div>
            <div class="card-body">
                <form asp-action="GasPrice" id="form" name="form1">
                    <div class="row">
                        <div class="col-md-6">
                            <label class="col-form-label">95</label>
                            <input type="number" value="@ViewData["95Price"]" name ="Price95"class="form-control"step="0.01">
                        </div>
                    </div>                    
                    <div class="row">
                        <div class="col-md-6">
                            <label class="col-form-label">91</label>
                            <input type="number" value="@ViewData["91Price"]" name ="Price91" class="form-control"step="0.01">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 " style="text-align: left; margin-top: 30px;">
                            <button type="submit" class="btn btn-dark btn-search" onclick=" return Valdation()"> Update </button>
                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>
</div>